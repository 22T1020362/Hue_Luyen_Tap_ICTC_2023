#include <bits/stdc++.h>
using namespace std;
int a,b;
int main()
{
    cin>>a>>b;
    int res=0;
    int i=1;
    while (i<=2){
        if (a>b){
            res+=a;
            a--;
        }
        else{
            res+=b;
            b--;
        }
        i++;
    }
    cout<<res;
    return 0;
}
